{
  "template": "purchases-*",
  "settings": {
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "_default_": {
      "_all": {
        "enabled": false
      }
    },
    "all": {
      "dynamic": false,
      "properties": {
        "store": {
          "type": "keyword"
        },
        "cost": {
          "type": "double"
        },
        "notes": {
          "type": "text"
        },
        "billtypekey": {
          "type": "integer"
        },
        "date": {
          "type": "date"
        }
      }
    }
  }
}
